extends ../layout 
//-업무능력 평가서 항목 수정//
block content 
    .container
      .table-responsive
        .table-wrapper
          .table-title
            .row
              .col-sm-8
                h2  동료 평가 항목 관리
              .col-sm-4
                button-group 
                  button.addButton(type='button' value='add' title='add') 항목추가
                  button.finishButton(type='button' value='completion' title='completion') 수정완료
          br
          table.table.table-bordered
            thead
              tr
                th 항목내용
                th 항목예시
                th Actions
            tbody
              if(items)
                each item in items
                  tr 
                    td.itemContent= item.item_title
                    td.itemExample= item.item_example
                    td
                      button.modifyButton(type='button' value='Edit' title='Edit') 수정
                      button.deleteButton(type='button' value='Delete' title='Delete') 삭제
              else 
                
